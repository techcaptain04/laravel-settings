{
    "name": "sajadsdi/laravel-setting-pro",
    "description": "Easy settings management for laravel framework",
    "type": "laravel-package",
    "keywords": [
        "laravel",
        "package",
        "settings",
        "configs"
    ],
    "license": "MIT",
    "autoload": {
        "files": [
            "src/Support/helpers.php"
        ],
        "psr-4": {
            "Sajadsdi\\LaravelSettingPro\\": "src/"
        }
    },
    "authors": [
        {
            "name": "SajaD SaeeDi",
            "email": "thunder11like@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-pdo": "*",
        "laravel/framework": "^9.0|^10.0",
        "sajadsdi/array-dot-notation": "^1.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sajadsdi\\LaravelSettingPro\\Providers\\LaravelSettingProServiceProvider"
            ]
        }
    }
}
